{
  "name": "latex",
  "rules": {
    "program": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_text_mode"
      }
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "comment_char"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "magic_comment"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "comment_text"
                },
                {
                  "type": "BLANK"
                }
              ]
            }
          ]
        }
      ]
    },
    "magic_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\s*"
        },
        {
          "type": "PATTERN",
          "value": "!T[eE]X"
        },
        {
          "type": "PATTERN",
          "value": "\\s+"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "comment_text"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "comment_text": {
      "type": "PATTERN",
      "value": ".+\\n"
    },
    "escape_char": {
      "type": "STRING",
      "value": "\\"
    },
    "begin_group": {
      "type": "STRING",
      "value": "{"
    },
    "end_group": {
      "type": "STRING",
      "value": "}"
    },
    "math_shift": {
      "type": "STRING",
      "value": "$"
    },
    "alignment_tab": {
      "type": "STRING",
      "value": "&"
    },
    "_end_of_line": {
      "type": "STRING",
      "value": "\n"
    },
    "parameter_token": {
      "type": "STRING",
      "value": "#"
    },
    "superscript": {
      "type": "STRING",
      "value": "^"
    },
    "subscript": {
      "type": "STRING",
      "value": "_"
    },
    "active_char": {
      "type": "STRING",
      "value": "~"
    },
    "comment_char": {
      "type": "STRING",
      "value": "%"
    },
    "_whitespace": {
      "type": "PATTERN",
      "value": "[\\n\\s\\t]"
    },
    "begin_inline_math": {
      "type": "STRING",
      "value": "$"
    },
    "begin_display_math": {
      "type": "STRING",
      "value": "$$"
    },
    "end_inline_math": {
      "type": "STRING",
      "value": "$"
    },
    "end_display_math": {
      "type": "STRING",
      "value": "$$"
    },
    "_text_mode": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "control_sequence"
        },
        {
          "type": "SYMBOL",
          "name": "text_group"
        },
        {
          "type": "SYMBOL",
          "name": "text"
        },
        {
          "type": "SYMBOL",
          "name": "environment"
        },
        {
          "type": "SYMBOL",
          "name": "verbatim"
        }
      ]
    },
    "text_group": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "begin_group"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_text_mode"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "end_group"
        }
      ]
    },
    "control_sequence": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "control_symbol"
        },
        {
          "type": "SYMBOL",
          "name": "control_word"
        }
      ]
    },
    "control_symbol": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "escape_char"
        },
        {
          "type": "SYMBOL",
          "name": "symbol"
        }
      ]
    },
    "control_word": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "escape_char"
        },
        {
          "type": "SYMBOL",
          "name": "letters"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "trailing_space"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "symbol": {
      "type": "PATTERN",
      "value": "[^a-zA-Z@]"
    },
    "letters": {
      "type": "PATTERN",
      "value": "[a-zA-Z@]+"
    },
    "trailing_space": {
      "type": "PATTERN",
      "value": "[\\s\\t\\n]+"
    },
    "verbatim": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "escape_char"
        },
        {
          "type": "STRING",
          "value": "verb"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_whitespace"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_verb"
        }
      ]
    },
    "text": {
      "type": "PATTERN",
      "value": "[^\\s\\n\\t\\#\\$\\%\\^\\&\\_\\{\\}\\~\\\\][^\\#\\$\\%\\^\\&\\_\\{\\}\\~\\\\]*"
    },
    "environment": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "open_env"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "env_body"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "close_env"
        }
      ]
    },
    "env_body": {
      "type": "SYMBOL",
      "name": "_text_mode"
    },
    "open_env": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "begin_command"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_whitespace"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "begin_group"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "_start_env_name"
          },
          "named": true,
          "value": "env_name"
        },
        {
          "type": "SYMBOL",
          "name": "end_group"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_whitespace"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "begin_command": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "escape_char"
        },
        {
          "type": "STRING",
          "value": "begin"
        }
      ]
    },
    "close_env": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "end_command"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_whitespace"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "begin_group"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "_end_env_name"
          },
          "named": true,
          "value": "env_name"
        },
        {
          "type": "SYMBOL",
          "name": "end_group"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_whitespace"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "end_command": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "escape_char"
        },
        {
          "type": "STRING",
          "value": "end"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "_start_env_name"
    },
    {
      "type": "SYMBOL",
      "name": "_end_env_name"
    },
    {
      "type": "SYMBOL",
      "name": "_erroneous_end_env_name"
    },
    {
      "type": "SYMBOL",
      "name": "_verb"
    }
  ],
  "inline": []
}